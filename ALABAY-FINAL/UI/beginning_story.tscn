theme_override_fonts/font = ExtResource("2_1ofhg")
text = "\"In the heart of Ilaya Likas, a tranquil village embraced by nature's bounty,
a sudden shadow falls. Mahalia, the revered Babaylan, the guardian of
wisdom and healing, lies gravely ill. The air is thick with worry as the villagers
gather under the guiding hand of Datu Kisig.\""
horizontal_alignment = 1
vertical_alignment = 1

[node name="paragraph2" type="Label" parent="."]
self_modulate = Color(1, 1, 1, 0)
layout_mode = 1
anchors_preset = 8
anchor_left = 0.5
