offset_right = 448.0
offset_bottom = 268.0
grow_horizontal = 2
grow_vertical = 2
theme_override_fonts/font = ExtResource("2_1ofhg")
text = "\"With blessings from the shaman and the support of your people, you
prepare for the journey. Armed with a map, a list of ingredients, and the
will to succeed, you set forth into the unknown. The fate of Mahalia, and the future of
Ilaya Likas, rest upon your shoulders.\""
horizontal_alignment = 1
vertical_alignment = 1

[node name="paragraph4" type="Label" parent="."]
