offset_top = -36.0
offset_right = 448.0
offset_bottom = 268.0
grow_horizontal = 2
grow_vertical = 2
theme_override_fonts/font = ExtResource("2_1ofhg")
text = "\"Among the gathered, one stands ready to answer the call. You, the Alabay,
an apprentice with much yet to learn, but with a heart full of courage and
determination. The task ahead is daunting: to travel to Talahib, gather the
rarest of ingredients, and craft a potion to save Mahalia.\""
horizontal_alignment = 1
vertical_alignment = 1

[node name="paragraph3" type="Label" parent="."]
self_modulate = Color(1, 1, 1, 0)
layout_mode = 1
anchors_preset = 8
